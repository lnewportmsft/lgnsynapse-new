{
	"name": "SQLServerFullLoad",
	"properties": {
		"activities": [
			{
				"name": "FullLoadAdventureWorksLT2017",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SqlServerSource",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "SqlServerTable",
						"type": "DatasetReference",
						"parameters": {
							"schemaName": "SalesLT",
							"tableName": "Product"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "DelimitedText1",
						"type": "DatasetReference",
						"parameters": {
							"folderPath": "AdventureWorksLT2017/Full",
							"filePath": "SalesLT.Product.csv",
							"triggerStart": {
								"value": "@pipeline().parameters.triggerStartTime",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
			"triggerStartTime": {
				"type": "string"
			}
		},
		"annotations": [],
		"lastPublishTime": "2021-12-01T03:41:00Z"
	},
	"type": "Microsoft.Synapse/workspaces/pipelines"
}